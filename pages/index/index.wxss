.container{width:100%;background:#f4f4f4}.container .header{position:fixed;top:0;background-color:#ffffff;height:100rpx;width:100%;border-top:1px solid #f5f5f5;border-bottom:1px solid #f5f5f5;display:flex;flex-direction:row;align-items:center;justify-content:center;box-shadow:0 0 3px 0 #e3e3e3;z-index:99}.container .header .box{display:flex;flex-direction:row;justify-content:center;align-items:center;border-radius:50px;background-color:#f5f5f5;height:68rpx;width:700rpx;color:#999999}.container .header .box image{margin-right:10px;width:14px;height:14px;margin-bottom:-2px}.container .header .box .input-title{font-size:.8rem}.container .swiper{margin-top:100rpx}.container .icon-wrapper{padding-top:40rpx;padding-bottom:20rpx;margin-bottom:20rpx;background:white;width:100%;display:grid;grid-template-columns:repeat(4, 25%);grid-template-rows:100rpx 100rpx;grid-row-gap:50rpx;justify-items:center}.container .hot-wrapper{background:white}.container .hot-wrapper .weui-panel__hd{display:flex;flex-direction:row;align-items:flex-end}.container .hot-wrapper .weui-panel__hd .fire{width:50rpx;height:50rpx}.container .hot-wrapper .weui-panel__hd:after{left:0}.container .tag{margin-right:14rpx}